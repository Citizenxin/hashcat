__kernel void m02710_s04 (__global pw_t *pws, __global const kernel_rule_t *rules_buf, __global const comb_t *combs_buf, __global const bf_t *bfs_buf, __global void *tmps, __global void *hooks, __global const u32 *bitmaps_buf_s1_a, __global const u32 *bitmaps_buf_s1_b, __global const u32 *bitmaps_buf_s1_c, __global const u32 *bitmaps_buf_s1_d, __global const u32 *bitmaps_buf_s2_a, __global const u32 *bitmaps_buf_s2_b, __global const u32 *bitmaps_buf_s2_c, __global const u32 *bitmaps_buf_s2_d, __global plain_t *plains_buf, __global const digest_t *digests_buf, __global u32 *hashes_shown, __global const salt_t *salt_bufs, __global const void *esalt_bufs, __global u32 *d_return_buf, __global u32 *d_scryptV0_buf, __global u32 *d_scryptV1_buf, __global u32 *d_scryptV2_buf, __global u32 *d_scryptV3_buf, const u32 bitmap_mask, const u32 bitmap_shift1, const u32 bitmap_shift2, const u32 salt_pos, const u32 loop_pos, const u32 loop_cnt, const u32 il_cnt, const u32 digests_cnt, const u32 digests_offset, const u32 combs_mode, const u32 gid_max)
{
  /**
   * modifier
   */

  const u32 gid = get_global_id (0);
  const u32 lid = get_local_id (0);
  const u32 lsz = get_local_size (0);

  /**
   * bin2asc table
   */

  __local u32 l_bin2asc[256];

  for (u32 i = lid; i < 256; i += lsz)
  {
    const u32 i0 = (i >> 0) & 15;
    const u32 i1 = (i >> 4) & 15;

    l_bin2asc[i] = ((i0 < 10) ? '0' + i0 : 'a' - 10 + i0) << 8
                 | ((i1 < 10) ? '0' + i1 : 'a' - 10 + i1) << 0;
  }

  barrier (CLK_LOCAL_MEM_FENCE);

  if (gid >= gid_max) return;

  /**
   * base
   */

  const pw_t pw = pws[gid];

  md5_ctx_t ctx_outer;

  md5_init (&ctx_outer);

  md5_update (&ctx_outer, pw.i, pw.pw_len);

  /**
   * salt
   */

  u32 s0[4];
  u32 s1[4];

  s0[0] = salt_bufs[salt_pos].salt_buf[0];
  s0[1] = salt_bufs[salt_pos].salt_buf[1];
  s0[2] = salt_bufs[salt_pos].salt_buf[2];
  s0[3] = salt_bufs[salt_pos].salt_buf[3];
  s1[0] = salt_bufs[salt_pos].salt_buf[4];
  s1[1] = salt_bufs[salt_pos].salt_buf[5];
  s1[2] = salt_bufs[salt_pos].salt_buf[6];
  s1[3] = salt_bufs[salt_pos].salt_buf[7];

  const int salt_len = 30;

  /**
   * digest
   */

  const u32 search[4] =
  {
    digests_buf[digests_offset].digest_buf[DGST_R0],
    digests_buf[digests_offset].digest_buf[DGST_R1],
    digests_buf[digests_offset].digest_buf[DGST_R2],
    digests_buf[digests_offset].digest_buf[DGST_R3]
  };

  /**
   * loop
   */

  for (u32 il_pos = 0; il_pos < il_cnt; il_pos += VECT_SIZE)
  {
    const comb_t comb = combs_buf[il_pos];

    md5_ctx_t ctx_inner = ctx_outer;

    md5_update (&ctx_inner, comb.i, comb.pw_len);

    md5_final (&ctx_inner);

    u32x a = ctx_inner.h[0];
    u32x b = ctx_inner.h[1];
    u32x c = ctx_inner.h[2];
    u32x d = ctx_inner.h[3];

    u32x h0[4];
    u32x h1[4];

    h0[0] = uint_to_hex_lower8 ((a >>  0) & 255) <<  0
          | uint_to_hex_lower8 ((a >>  8) & 255) << 16;
    h0[1] = uint_to_hex_lower8 ((a >> 16) & 255) <<  0
          | uint_to_hex_lower8 ((a >> 24) & 255) << 16;
    h0[2] = uint_to_hex_lower8 ((b >>  0) & 255) <<  0
          | uint_to_hex_lower8 ((b >>  8) & 255) << 16;
    h0[3] = uint_to_hex_lower8 ((b >> 16) & 255) <<  0
          | uint_to_hex_lower8 ((b >> 24) & 255) << 16;
    h1[0] = uint_to_hex_lower8 ((c >>  0) & 255) <<  0
          | uint_to_hex_lower8 ((c >>  8) & 255) << 16;
    h1[1] = uint_to_hex_lower8 ((c >> 16) & 255) <<  0
          | uint_to_hex_lower8 ((c >> 24) & 255) << 16;
    h1[2] = uint_to_hex_lower8 ((d >>  0) & 255) <<  0
          | uint_to_hex_lower8 ((d >>  8) & 255) << 16;
    h1[3] = uint_to_hex_lower8 ((d >> 16) & 255) <<  0
          | uint_to_hex_lower8 ((d >> 24) & 255) << 16;

    md5_ctx_t ctx2;

    md5_init (&ctx2);

    ctx2.w0[0] = h0[0];
    ctx2.w0[1] = h0[1];
    ctx2.w0[2] = h0[2];
    ctx2.w0[3] = h0[3];
    ctx2.w1[0] = h1[0];
    ctx2.w1[1] = h1[1];
    ctx2.w1[2] = h1[2];
    ctx2.w1[3] = h1[3];
    ctx2.w2[0] = s0[0];
    ctx2.w2[1] = s0[1];
    ctx2.w2[2] = s0[2];
    ctx2.w2[3] = s0[3];
    ctx2.w3[0] = s1[0];
    ctx2.w3[1] = s1[1];
    ctx2.w3[2] = s1[2];
    ctx2.w3[3] = s1[3];

    ctx2.len = 32 + salt_len;

    md5_final (&ctx2);

    a = ctx2.h[0];
    b = ctx2.h[1];
    c = ctx2.h[2];
    d = ctx2.h[3];

    COMPARE_S_SIMD (a, d, c, b);
  }
}

